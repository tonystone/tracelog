source 'https://github.com/CocoaPods/Specs.git'

use_frameworks!

install! 'cocoapods', :deterministic_uuids => false

target 'TraceLog-iOS' do
   platform :ios, '8.0'
    
   pod "TraceLog", :path => "../"
end

target 'TraceLog-iOS-Tests' do
   platform :ios, '8.0'
  
   pod "TraceLog", :path => "../"
   pod "TraceLog/ObjC", :path => "../"
end

target 'TraceLog-OSX' do
   platform :osx, '10.12'
  
   pod "TraceLog", :path => "../"
end

target 'TraceLog-OSX-Tests' do
   platform :osx, '10.12'
    
   pod "TraceLog", :path => "../"
   pod "TraceLog/ObjC", :path => "../"
end
